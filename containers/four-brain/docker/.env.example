# Orchestrator Hub environment (.env example)
# Copy to .env and fill in real secrets before running `docker compose up -d orchestrator-hub`

# Core
BRAIN_ROLE=orchestrator
PORT=9098
PHASE1_SMOKE=0
TRITON_URL=http://triton:8000
REDIS_URL=redis://redis:6379/4

# Moonshot/K2
# For Kimi/Moonshot API access
K2_API_URL=https://api.moonshot.cn/v1/chat/completions
K2_MODEL=kimi-thinking-preview
K2_API_KEY=

# Supabase (optional but recommended)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_ANON_KEY=

# Postgres (if orchestrator talks directly)
POSTGRES_URL=

# GPU settings (this container is CPU-only by default)
NVIDIA_VISIBLE_DEVICES=none
CUDA_VISIBLE_DEVICES=
TORCH_CUDA_ARCH_LIST=12.0
CUDA_LAUNCH_BLOCKING=1
TORCH_COMPILE=1
CUDA_GRAPHS=1
TORCHDYNAMO_FULLGRAPH=1
NCCL_P2P_DISABLE=1

